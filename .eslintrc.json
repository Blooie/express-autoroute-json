{
    "env": {
        "node": true
    },
    "extends": "airbnb",
    "rules": {
      "no-var": 0,
      "prefer-template": 0,
      "space-before-function-paren": 0,
      "object-shorthand": 0,
      "new-cap": [2 , { "capIsNewExceptions": ["Q", "ObjectId"] }],
      "prefer-arrow-callback": 0,
      "func-names": 0,
      "no-underscore-dangle": [2, { "allow": ["_id"] }]
    }
}
